{
  "family": "llmopstdlive",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::019922908355:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "llmops-live-container",
      "image": "019922908355.dkr.ecr.eu-north-1.amazonaws.com/llmopsrepolive",
      "cpu": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "name": "llmops-live-container-8080-tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "ENV",
          "value": "production"
        },
        {
          "name": "OPENROUTER_API_KEY",
          "value": "PLACEHOLDER"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/llmopstdlive",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
